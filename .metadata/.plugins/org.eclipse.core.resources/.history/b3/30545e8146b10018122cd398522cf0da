package com.spring.model;

import javax.persistence.Entity;
import javax.persistence.Table;

/*================================================================================================
 * POLICY CLASS
 * -----------------------------------------------------------------------------------------------
 * 
 * 
 *================================================================================================ 
 */

@Entity
@Table(name="assinsurance_policy_tbl")
public class Policy 
{
	@Id
	private int id;
	private String policyNum, firstName, lastName;
	private boolean status;
}
